{
  "name": "Google Home",
  "version": "2.0.9",
  "slug": "google_home_local",
  "description": "Google Home to Home Assistant manager",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "boot": "auto",
  "init": false,
  "panel_icon": "mdi:google-home",
  "options": {
    "firebase": {
      "apiKey": "",
      "databaseURL": ""
    },
    "googleJwtPath": "/config/google-local/serviceAccountKey.json"
  },
  "schema": {
    "firebase": {
      "apiKey": "str",
      "databaseURL": "url"
    },
    "googleJwtPath": "str"
  },
  "ports": {
    "8088/tcp": 8088,
    "3311/udp": 3311
  },
  "ports_description": {
    "8088/tcp": "API Server",
    "3311/udp": "Google local Descovery port"
  },
  "map": ["config:rw"],
  "homeassistant_api": true,
  "hassio_api": true,
  "ingress": true,
  "ingress_port": 8088,
  "ingress_entry": ""
}
