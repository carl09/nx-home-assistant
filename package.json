    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "prettier": "2.1.2",
    "pretty-quick": "^2.0.1",
    "rxjs-tslint": "^0.1.8",
    "rxjs-tslint-rules": "^4.34.0",
    "ts-jest": "26.4.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "tslint-angular": "^3.0.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tsutils": "^3.17.1",
    "typescript": "4.0.5"
