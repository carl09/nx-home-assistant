    "@mdi/angular-material": "^5.4.55",
    "@ngrx/effects": "10.0.0",
    "@ngrx/entity": "10.0.0",
    "@ngrx/router-store": "10.0.0",
    "@ngrx/store": "10.0.0",
    "@nrwl/angular": "11.0.20",
    "actions-on-google": "^2.12.0",
    "cbor": "^5.0.2",
    "core-js": "^2.5.4",
    "express": "4.17.1",
    "firebase": "^7.17.1",
    "firebase-admin": "^8.13.0",
