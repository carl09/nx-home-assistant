{
  "name": "nx-home-assistant",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "main": "dist/apps/functions/main.js",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "_start": "ng serve",
    "start": "nx run-many --target=serve --projects=web-admin,google-local --parallel --maxParallel=2",
    "build": "ng build --configuration production --output-hashing none && ng build google-local --configuration production --source-map false",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "firebase:deploy": "firebase deploy --only functions",
    "prettier": "pretty-quick --staged",
    "_postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "dependencies": {
    "@angular/animations": "12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/forms": "12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "12.2.16",
    "@angular/platform-browser-dynamic": "12.2.16",
    "@angular/router": "12.2.16",
    "@google/local-home-sdk": "^1.4.1",
    "@mdi/angular-material": "^5.9.55",
    "@ngrx/effects": "12.5.1",
    "@ngrx/entity": "12.5.1",
    "@ngrx/router-store": "12.5.1",
    "@ngrx/store": "12.5.1",
    "@nrwl/angular": "12.0.8",
    "actions-on-google": "^2.13.0",
    "cbor": "^7.0.5",
    "core-js": "^2.6.12",
    "express": "4.17.1",
    "firebase": "^8.4.2",
    "firebase-admin": "^9.6.0",
    "firebase-functions": "^3.13.2",
    "lodash": "^4.17.21",
    "rxjs": "^6.6.7",
    "shortid": "^2.2.16",
    "tslib": "^1.14.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.18",
    "@angular/cli": "12.2.18",
    "@angular/compiler-cli": "12.2.16",
    "@angular/language-service": "12.2.16",
    "@ngrx/schematics": "12.5.1",
    "@ngrx/store-devtools": "12.5.1",
    "@nrwl/express": "12.0.8",
    "@nrwl/jest": "12.0.8",
    "@nrwl/node": "12.0.8",
    "@nrwl/workspace": "12.0.8",
    "@types/cbor": "^5.0.1",
    "@types/jest": "26.0.8",
    "@types/lodash": "^4.14.168",
    "@types/node": "14.14.33",
    "@types/shortid": "0.0.29",
    "@types/ws": "^7.4.1",
    "codelyzer": "6.0.0",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "husky": "^4.3.8",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.2",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "rxjs-tslint": "^0.1.8",
    "rxjs-tslint-rules": "^4.34.7",
    "ts-jest": "26.4.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "tslint-angular": "^3.0.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tsutils": "^3.21.0",
    "typescript": "4.3.5"
  },
  "husky": {
    "hooks": {
      "_pre-commit": "npm run prettier"
    }
  }
