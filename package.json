    "@angular/language-service": "11.0.3",
    "@ngrx/schematics": "10.0.0",
    "@ngrx/store-devtools": "10.0.0",
    "@nrwl/express": "11.0.20",
    "@nrwl/jest": "11.0.20",
    "@nrwl/node": "11.0.20",
    "@nrwl/workspace": "11.0.20",
    "@types/cbor": "^5.0.0",
    "@types/jest": "26.0.8",
    "@types/lodash": "^4.14.158",
    "@types/node": "12.12.38",
